  <script>
	  $(document).ready(function () {
		  $('table.responsive').DataTable( {
			  //scrollY: 300,
			  paging: false,
			  lengthMenu: false,
			  info: false,
			  searching: false,
			  responsive: {
				details: { type: 'column'}
			  },
			  "order": [[ 1, 'asc' ]],
			  columnDefs: [
				{ className: 'control', orderable: false, targets: 0 },
				{ responsivePriority: 0, targets: 1 },
				{ responsivePriority: 1, targets: -1 },
				{ "orderable": false, "targets": -1 }
			 ]
		  }); // to get all responsive tables sorted by order. be sure to trigger by ID if each table has diffrent sort column
		  
		  $('[data-toggle="tooltip"]').tooltip(); // to include tooltip
		  $("select.multiselect").bsMultiSelect(); // to include multi-select dropdown selector
		   var editor = new FroalaEditor('div.editTextDiv'); //to include text editor
		  window.setTimeout(function() {
				$(".alert-success").fadeTo(4000, 0).slideUp(4000, function(){
					$(this).addClass('hide'); 
				});
			}, 5000); //to get save messages to disppear on its own 
    
     $('#fileDescriptionInput').keyup(function () {
          var max = 256;
          var char = 0; 
          var len = $(this).val().length;
          if (len >= max) {
            var char = len - max; 
            $('.fdt').addClass('iq-input-error-msg'); 
            $('#charNum').html(len + '/256. You have exceeded the character limit by ' + char + ' characters.' );
          } else {
            $('#charNum').html(len + '/256.');
          }
        }); // to include character limit. Make sure you have the ID replaced with your own textarea ID OR use a class name if multiple
	  });
